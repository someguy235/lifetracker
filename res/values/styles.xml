<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <color name="black">#000000</color>
    <color name="white">#FFFFFF</color>
    
    <color name="default_background">#001C24</color>
    
    <color name="tile_default">@color/lt_blue</color>
	<color name="tile_changed">@color/lt_red</color>
	
	<color name="save_default">@color/black</color>
	<color name="save_changed">@color/lt_red</color>
	<color name="save_saved">@color/lt_green</color>
	
	<color name="lt_red">#dc322f</color>
	<color name="lt_orange">#cb4b16</color>
	<color name="lt_cyan">#2aa198</color>	
	<color name="lt_yellow">#b58900</color>
	<color name="lt_green">#859900</color>
    <color name="lt_blue">#268bd2</color>
	<color name="lt_violet">#6c71c4</color>
	<color name="lt_magenta">#d33682</color>
	
	<color name="pale_red">#E8A5A9</color>
	<color name="pale_green">#A7CFB1</color>
	
	
</resources>
